{
  "MetricsOptions": {
    "DefaultContextLabel": "Mitchell.CSG.apptest",
    "Enabled": true,
    "ReportingEnabled": false
  },
  "MetricsWebTrackingOptions": {
    "ApdexTrackingEnabled": true,
    "ApdexTSeconds": 0.1,
    "IgnoredHttpStatusCodes": [ 404 ],
    "IgnoredRoutesRegexPatterns": [],
    "OAuth2TrackingEnabled": true
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": false,
    "EnvironmentInfoEndpointEnabled": false
  },
  "MetricReportingOptions": {
    "IntervalSeconds": 60
  },
  "eureka": {
    "client": {
      "shouldFetchRegistry": false
    },
    "instance": {
      "healthCheckUrlPath": "/admin/health",
      "metadataMap": {
        "managementContextPath": "/admin",
        "smoketestPath": "/admin/smoketest",
        "gatewayPath": "csg/apptest",
        "smoketestEnabled": true
      }
    }
  },
  "spring": {
    "cloud": {
      "config": {
        "name": "csg_apptest",
        "failFast": false
      }
    }
  },
  "management": {
    "endpoints": {
      "path": "/admin"
    }
  },
  "SwaggerSettings": {
    "name": "apptest Support",
    "email": "Fusionists_ScrumTeam@mitchell.com",
    "url": "https://confluence.corp.int/display/CSGUITeam/Starter+App+Template"
  },
  "spa": "react",
  "spaPath": "UI.React"
}